 CREATE TABLE CANDIDATE (
    CAND_ID INT PRIMARY KEY IDENTITY(1,1),NAME VARCHAR(100),LASTNAME VARCHAR(100),USERNAME VARCHAR(50),STREAM VARCHAR(200),BRANCH VARCHAR(200),
BATCH_YEAR INT,COLLEGE VARCHAR(500),COMPANY VARCHAR(200),
DESIGNATION VARCHAR(50),EMAIL_ID VARCHAR(200),LINKEDIN_LINK VARCHAR(500), LOCATION VARCHAR(200),
SKILL1 VARCHAR(100),ABOUT VARCHAR(200),IMAGE_URL VARCHAR(200));
ALTER TABLE CANDIDATE
ADD Status VARCHAR(50) NULL;
ALTER TABLE CANDIDATE ADD CONSTRAINT UQ_Candidate_Username UNIQUE (USERNAME);
ALTER TABLE CANDIDATE
ADD DEFAULT 'Accepted' FOR Status;
 DROP TABLE STUDENT
-- Create the STUDENT table with a persisted computed column
CREATE TABLE STUDENT (
    ID INT IDENTITY(1,1),
    STUDENT_ID AS ('SD' + CAST(ID AS VARCHAR(8000))) PERSISTED, ENROLLMENT_NUMBER varchar(10), 
    NAME VARCHAR(100),
    LASTNAME VARCHAR(100),
    USERNAME VARCHAR(50),
    PASSWORD VARCHAR(8),
    STREAM VARCHAR(200),
    BRANCH VARCHAR(200),
    STARTING_YEAR INT,
    ENDING_YEAR INT,
    COLLEGE VARCHAR(500),
    EMAIL_ID VARCHAR(200),
    LINKEDIN_LINK VARCHAR(500),
    SKILL1 VARCHAR(100),
    SKILL2 VARCHAR(100),
    SKILL3 VARCHAR(100),
    ABOUT VARCHAR(200),
    IMAGE_URL NVARCHAR(200),
    CONSTRAINT PK_STUDENT PRIMARY KEY (ID) -- Define primary key on STUDENT_ID
);  
 USE YUVA_CONNECT
 CREATE TABLE VOTE (
    VOTE_ID INT PRIMARY KEY IDENTITY(1,1),
    STUDENT_ID INT NOT NULL,
    CAND_ID INT NOT NULL,
    FOREIGN KEY (STUDENT_ID) REFERENCES STUDENT(ID),
    FOREIGN KEY (CAND_ID) REFERENCES CANDIDATE(CAND_ID)
);

CREATE TABLE ELECTION (
    ELECTION_ID INT PRIMARY KEY IDENTITY(1,1),
    IS_ACTIVE BIT NOT NULL DEFAULT 0,
    START_DATE DATETIME,
    END_DATE DATETIME
);

INSERT INTO ELECTION (IS_ACTIVE, START_DATE, END_DATE)
VALUES (0, GETDATE(), NULL); 


CREATE TABLE RESULT (
    RESULT_ID INT PRIMARY KEY IDENTITY(1,1),
    ISDECLARED BIT NOT NULL DEFAULT 0,
    WINNER_CARD_ID INT,
    DECLARED_DATE DATETIME,
    FOREIGN KEY (WINNER_CARD_ID) REFERENCES CANDIDATE(CAND_ID)
);

INSERT INTO RESULT (ISDECLARED, WINNER_CARD_ID, DECLARED_DATE)
VALUES (0, NULL, NULL);

 

