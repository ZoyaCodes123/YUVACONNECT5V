/*NEW ALUMNI TABLE*/ 
SELECT * FROM NEW_ALUMNI
SELECT * FROM ALUMNI
CREATE TABLE NEW_ALUMNI(ID INT IDENTITY(1,1),
ALUMNI_ID AS ('AL'+CAST(ID AS VARCHAR(MAX))), 
NAME VARCHAR(100),LASTNAME VARCHAR(100),USERNAME VARCHAR(50),
PASSWORD VARCHAR(8),STREAM VARCHAR(200),BRANCH VARCHAR(200),
BATCH_YEAR INT,COLLEGE VARCHAR(500),COMPANY VARCHAR(200),
DESIGNATION VARCHAR(50),EMAIL_ID VARCHAR(200),LINKEDIN_LINK VARCHAR(500), LOCATION VARCHAR(200),
SKILL1 VARCHAR(100), SKILL2 VARCHAR(100), SKILL3 VARCHAR(100),ABOUT VARCHAR(200),IMAGE_URL VARCHAR(200));


USE YUVA_CONNECT 
/*ALUMNI TABLE*/ 
CREATE TABLE ALUMNI(ID INT IDENTITY(1,1),
ALUMNI_ID AS ('AL'+CAST(ID AS VARCHAR(MAX))), ENROLLMENT_NUMBER VARCHAR(10),
NAME VARCHAR(100),LASTNAME VARCHAR(100),USERNAME VARCHAR(50),
