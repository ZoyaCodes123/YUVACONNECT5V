/*NEW_STUDENT TABLE*/
CREATE TABLE NEW_STUDENT (ID INT IDENTITY(1,1),STUDENT_ID AS('SD' +CAST(ID AS VARCHAR(MAX))), ENROLLMENT_NUMBER varchar(10), NAME VARCHAR(100), LASTNAME VARCHAR(100), USERNAME VARCHAR(50), 
PASSWORD VARCHAR(8),STREAM VARCHAR(200), BRANCH VARCHAR(200), STARTING_YEAR INT, ENDING_YEAR INT, COLLEGE VARCHAR(500), EMAIL_ID VARCHAR(200), 
LINKEDIN_LINK VARCHAR(500), SKILL1 VARCHAR(100), SKILL2 VARCHAR(100), SKILL3 VARCHAR(100), ABOUT VARCHAR(200), IMAGE_URL NVARCHAR(200))

/*STUDENT TABLE*/
