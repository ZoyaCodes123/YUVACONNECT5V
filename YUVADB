/*EVENT TABLE*/

CREATE TABLE EVENTS (
    ID INT PRIMARY KEY IDENTITY(1,1),
    EVENT_ID AS ('EV' + CAST(ID AS VARCHAR(MAX))),
    EVENT_NAME VARCHAR(100),
    STREAM VARCHAR(50),
    BRANCH VARCHAR(50),
    LOCATION VARCHAR(100),
    INFO VARCHAR(200),
    STARTDATE DATE,
    ENDDATE DATE,
    STARTTIME TIME(0),
    ENDTIME TIME(0),
    STARTDAY VARCHAR(20),
    ENDDAY VARCHAR(20),
    IMAGEURL NVARCHAR(255),EVENT_TYPE VARCHAR(50),ORGANIZER_TYPE VARCHAR(7),ORGANIZER_NAME VARCHAR(100),LINK VARCHAR(1000)
);
